<% @coding_resources.each do |resource|%>
<div class="booyah-box col-10 offset-1">
  <h1><%= resource.title %></h1>
    <i>Programming Language: <%= resource.programming_language %></i><br /><br />
    <p>Classification: <%= resource.classification %></p><br />
    <p>Resource Type: <%= resource.resource_type %></p><br />
    <p>Description: <%= resource.description %></p><br />
</div>

<% end %>